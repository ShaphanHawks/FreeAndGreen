# Twilio SMS Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Alternative Twilio env var names (if using different naming)
TWI_SID=your_twilio_account_sid
TWI_TOKEN=your_twilio_auth_token
TWI_PHONE=+1234567890

# Admin Credentials
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=secure_admin_password

# Session Secret (change this in production)
SESSION_SECRET=your_secure_session_secret_here_change_in_production

# Database URL (for PostgreSQL if switching from SQLite)
DATABASE_URL=postgresql://username:password@localhost:5432/pickup_scheduler
